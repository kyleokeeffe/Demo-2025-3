@model List<demo2025_razor.ViewModels.QuoteViewModel>
<table id="tblQuotes">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Renter</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var quote in Model)
        {
            <tr>
                <td>@quote.Id</td>
                <td>@quote.Name</td>
                <td>
                    <select id="selectQuotes">
                        <option></option>
                    </select>

                   
            </tr>
        }
    </tbody>
</table>